					<node uid="09dd1531-af0c-4e94-a64f-3803cb51a70e" class="de.saar.coli.dialogos.marytts.plugin.TTSNode">
						<graphics x="332" y="348" fill="#00b200"/>
						<att name="title" type="string" value="Sprachsynthese"/>
						<att name="prompt" type="text">
							<value>&quot;Möchtest du das Rezept für &quot; + term2 + term3 + term1 + &quot; umrechnen?&quot;</value>
						</att>
						<att name="prompt_type" type="string" value="expression"/>

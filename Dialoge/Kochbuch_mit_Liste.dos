		<node uid="d0f420d7-b218-4fcf-a38a-59f8df29fcb9" class="com.clt.diamant.graph.nodes.ScriptNode">
			<graphics x="3" y="533" fill="#99ffff"/>
			<att name="title" type="string" value="Dynamische Grammatik"/>
			<att name="comment" type="text">
				<value>Das, was mal (so ungefähr) passieren soll.</value>
			</att>
			<att name="script" type="text">
				<value>dyngram = &quot;root $input; \n&quot;;
dyngram = dyngram + &quot;$input = $zutaten {$=[\&quot;zutaten\&quot;, $term_z]} | $eigenschaft | $anleitung; \n&quot;;
dyngram = dyngram + &quot;$zutaten = Wie viel $term_z brauche ich ; \n&quot;;

#dyngram = &quot;$term_z = milch | zucker; \n&quot;;
for (int i=1; i&lt;len(ingredients); i++)
	dyngram = dyngram + &quot;|&quot;+ ingredients[i];
//
dyngram = dyngram + &quot;$eigenschaft = Wie lange (braucht | dauert) {$=[\&quot;eigenschaft\&quot;,\&quot;zubereitungszeit\&quot;]} | (Wie schwer ist | Welchen Schwierigkeitsgrad) {$=[\&quot;eigenschaft\&quot;,\&quot;schwierigkeitsgrad\&quot;]}; \n&quot;;
dyngram = dyngram + &quot;$anleitung = Was ist der $term_a Schritt | $term_a Schritt | (Wiederhole den Schritt | [diesen Schritt] nochmal {$=\&quot;anleitung\&quot;,\&quot;repeat\&quot;}); \n&quot;;
dyngram = dyngram + &quot;$term_a = nächste {$=\&quot;anleitung\&quot;,\&quot;next\&quot;} | letzte {$=\&quot;anleitung\&quot;,\&quot;previous\&quot;}; \n&quot;;


#Dinge escapen!!! 
# -&gt; \&quot;


#grammatik = grammatik + &quot;root $Eingabe; \n&quot;;
#grammatik = grammatik + &quot;$Eingabe = $Zahlen | Ende \n;&quot;;
#grammatik = grammatik + &quot;$Zahlen = 0 &quot;;
#for (int i=1; i&lt;999; i++)
#  grammatik = grammatik + &quot;|&quot;+i;
#grammatik = grammatik + &quot;;&quot;;</value>
			</att>
		</node>
		<node uid="d1129d90-47a0-4f40-bb9b-3bab7646b39a" class="com.clt.diamant.graph.nodes.EndNode">
			<graphics x="337" y="167" fill="#9999ff"/>
			<att name="title" type="string" value="Ende"/>
		</node>
		<node uid="d6849f9b-ad6c-47b8-9212-f6f0655d0379" class="com.clt.diamant.graph.nodes.ScriptNode">
			<graphics x="833" y="210" fill="#99ffff"/>
			<att name="title" type="string" value="Grammatik"/>
			<att name="comment" type="text">
				<value>Die Grammatik ist sehr hässlich, da einzeilig
</value>
			</att>
			<att name="script" type="text">
				<value>dyngram = &quot;root $input; \n&quot;;
dyngram = dyngram + &quot;$input = (ende | fertig | abbrechen | Programm schließen) {$=\&quot;ende\&quot;} | (wiederholen | nochmal) {$=\&quot;wiederholen\&quot;} | $ruhezeit {$=[\&quot;eigenschaft\&quot;,\&quot;Ruhezeit\&quot;]} | $arbeitszeit {$=[\&quot;eigenschaft\&quot;,\&quot;Arbeitszeit\&quot;]} | $kochzeit {$=[\&quot;eigenschaft\&quot;,\&quot;Koch-/Backzeit\&quot;]} |$gesamtzeit {$=[\&quot;eigenschaft\&quot;, \&quot;Gesamtzeit\&quot;]} | $schwierig {$=[\&quot;eigenschaft\&quot;,\&quot;Schwierigkeitsgrad\&quot;]} | $kalorien {$=[\&quot;eigenschaft\&quot;,\&quot;Kalorien\&quot;]} | $anleitung {$=$anleitung} | $zutaten_menge {$=$zutaten_menge} | $zutaten_exist {$=$zutaten_exist} | $zutaten {$=\&quot;zutaten\&quot;} | $anleitung_ganz {$=\&quot;anleitung\&quot;} | $einkaufszettel {$=\&quot;einkaufszettel\&quot;};&quot;;
dyngram = dyngram + &quot;$ruhezeit = wie viel Ruhezeit (muss | kann | soll) ich rechnen | was ist die Ruhezeit | wie lange muss es ruhen | [die] Ruhezeit;&quot;;
dyngram = dyngram + &quot;$arbeitszeit = wie viel Arbeitszeit (muss | kann | soll) ich rechnen | was ist die Arbeitszeit | [die] Arbeitszeit;&quot;;
dyngram = dyngram + &quot;$kochzeit = wie viel (Kochzeit | Backzeit) (muss | kann | soll) ich rechnen | was ist die (Kochzeit | Backzeit) | wie lange (kocht | backt) es | wie lange muss es (kochen | backen) | [die] Kochzeit | [die] Backzeit;&quot;;
dyngram = dyngram + &quot;$gesamtzeit = wie lange brauche ich | wie lange dauert das | wie ist die Gesamtzeit | wie viel Zeit muss ich (rechnen | einplanen) | [die] Gesamtzeit;&quot;;
dyngram = dyngram + &quot;$schwierig = wie schwer ist (es | das Rezept) | welchen Schwierigkeitsgrad hat (es | das Rezept)| Schwierigkeitsgrad | (wie | was) ist der Schwierigkeitsgrad | Schwierigkeitsgrad | ist es schwierig ;&quot;;
dyngram = dyngram + &quot;$kalorien = wie viele Kalorien hat das (Gericht | Essen);&quot;;

dyngram = dyngram + &quot;$anleitung = $next {$=[\&quot;anleitung\&quot;,\&quot;next\&quot;]} | $prev {$=[\&quot;anleitung\&quot;,\&quot;previous\&quot;]} | $first {$=[\&quot;anleitung\&quot;,\&quot;first\&quot;]} | $rep {$=[\&quot;anleitung\&quot;,\&quot;repeat\&quot;]} | $laststep {$=[\&quot;anleitung\&quot;, \&quot;last\&quot;]};&quot;;
dyngram = dyngram + &quot;$next = was ist der nächste Schritt | nächster Schritt | weiter ;&quot;;
dyngram = dyngram + &quot;$laststep = was ist der letzte Schritt | letzter Schritt | gehe ans Ende | was muss ich als letztes machen;&quot;;
dyngram = dyngram + &quot;$prev = was war der (letzte | vorherige) Schritt;&quot;;
dyngram = dyngram + &quot;$first = erster Schritt | von vorne | was ist der erste Schritt | wie geht es los;&quot;;

dyngram = dyngram + &quot;$rep = was ist der erste Schritt | [Wiederhole (den | diesen) Schritt] nochmal;&quot;;

dyngram = dyngram + &quot;$zutaten_exist = (brauche ich | braucht man | brauchen wir) $term_v {$=[\&quot;exists_zutat\&quot;, $term_v]} | (kommt | kommen | gehört | gehören) $term_v (rein | hinein | dazu | dabei) {$=[\&quot;exists_zutat\&quot;, $term_v]} | ist $term_z im Gericht {$=[\&quot;exists_zutat\&quot;, $term_v]} | (gehört | gehören) $term_v zu den Zutaten {$=[\&quot;exists_zutat\&quot;, $term_v]};&quot;;
dyngram = dyngram + &quot;$term_v = &quot; + get(zutaten_voc,0);
for (int i = 1; i&lt;length(zutaten_voc); i++)
  dyngram = dyngram + &quot; | &quot; + get(zutaten_voc,i);
dyngram = dyngram + &quot;;&quot;;

dyngram = dyngram + &quot;$zutaten_menge = wie viel $term_z [brauche ich] {$=[\&quot;zutaten\&quot;, $term_z]};&quot;;
dyngram = dyngram + &quot;$term_z = \&quot;&quot; + get(ingredients,0) + &quot;\&quot;&quot;;
for (int i = 1; i&lt;length(ingredients); i++)
  dyngram = dyngram + &quot; | \&quot;&quot; + get(ingredients,i) + &quot;\&quot;&quot;;
dyngram = dyngram + &quot;;&quot;;
dyngram = dyngram + &quot;$zutaten = [sag mir | gib mir] (die Liste | alle Zutaten | die Zutatenliste) | [die] Zutaten | [die] Zutatenliste | was brauche ich | Zutaten vorlesen;&quot;;
dyngram = dyngram + &quot;$anleitung_ganz = [sag mir | gib mir] ([alle] Schritte | [die] (komplette | ganze) Anleitung) | was muss ich tun | Anleitung | wie geht das Rezept | wie sind die Schritte;&quot;;
dyngram = dyngram + &quot;$einkaufszettel = [schreib mir einen] Einkaufszettel | [einen] Einkaufszettel schreiben | [eine] Einkaufsliste schreiben;&quot;;</value>
			</att>
		</node>

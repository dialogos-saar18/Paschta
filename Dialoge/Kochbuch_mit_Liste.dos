					<node uid="0fb72173-4152-4ae7-9891-f77484e7750a" class="com.clt.diamant.graph.nodes.ScriptNode">
						<graphics x="351" y="116" fill="#99ffff"/>
						<att name="title" type="string" value="Script"/>
						<att name="script" type="text">
							<value>gram = &quot;root $input; \n&quot;;
gram = gram + &quot;$input = $personen {$=$personen} | $zutaten {$=$zutaten} | (ende | abbrechen | gar nicht | nein | nicht umrechnen) {$=\&quot;Ende\&quot;};&quot;;
gram = gram + &quot;$personen = [ich möchte] [für] 1 [Person | Portion] kochen {$=[\&quot;Personen\&quot;,\&quot;&quot;+1+&quot;\&quot;]} | [ich möchte] [für|mit] $anz [Personen| Portionen] [kochen] {$=[\&quot;Personen\&quot;, $anz]} | für $anz {$=[\&quot;Personen\&quot;, $anz]};&quot;;
gram = gram + &quot;$anz = 2&quot;;
for (int i = 3; i&lt;50 ; i++)

					<node uid="60fe4a31-02e4-4975-b726-f83d311d9324" class="com.clt.diamant.graph.nodes.ScriptNode">
						<graphics x="323" y="377" fill="#99ffff"/>
						<att name="title" type="string" value="Grammatik (dyngram)"/>
						<att name="comment" type="text">
							<value>
</value>
						</att>
						<att name="script" type="text">
							<value>dyngram = &quot;root $input; \n&quot;;
//$input
dyngram = dyngram + &quot;$input = $ende {$=\&quot;ende\&quot;} | $wiederholen {$=\&quot;wiederholen\&quot;} | $ruhezeit {$=[\&quot;eigenschaft\&quot;,\&quot;Ruhezeit\&quot;]} | $arbeitszeit {$=[\&quot;eigenschaft\&quot;,\&quot;Arbeitszeit\&quot;]} | $kochzeit {$=[\&quot;eigenschaft\&quot;,\&quot;Koch-/Backzeit\&quot;]} |$gesamtzeit {$=[\&quot;eigenschaft\&quot;, \&quot;Gesamtzeit\&quot;]} | $schwierig {$=[\&quot;eigenschaft\&quot;,\&quot;Schwierigkeitsgrad\&quot;]} | $kalorien {$=[\&quot;eigenschaft\&quot;,\&quot;Kalorien\&quot;]} | $anleitung {$=$anleitung} | $zutaten_menge {$=$zutaten_menge} | $zutaten_exist {$=$zutaten_exist} | $zutaten {$=\&quot;zutaten\&quot;} | $anleitung_ganz {$=\&quot;anleitung\&quot;} | $einkaufszettel {$=\&quot;einkaufszettel\&quot;} |$help {$=\&quot;hilfe\&quot;};&quot;;


//$ende
dyngram = dyngram + &quot;$ende = ende | fertig | abbrechen | [Programm] schließen | schluss | beenden | aufhören | [das] reicht | nichts [mehr] | hör auf | mach schluss | sei (ruhig | leise | still) | halt | stop | [gut] ich bin fertig | das wars;&quot;;

//$wiederholen
dyngram = dyngram + &quot;$wiederholen = wiederholen | nochmal | wiederhol das bitte | (wie | was) [war das] [bitte];&quot;;

//eigenschaften

//$ruhezeit
dyngram = dyngram + &quot;$ruhezeit = wie viel Ruhezeit (muss | kann | soll) ich rechnen | was ist die Ruhezeit | wie lange muss es ruhen | [die] Ruhezeit;&quot;;
//$arbeitszeit
dyngram = dyngram + &quot;$arbeitszeit = wie viel Arbeitszeit (muss | kann | soll) ich rechnen | was ist die Arbeitszeit | [die] Arbeitszeit;&quot;;
//$kochzeit
dyngram = dyngram + &quot;$kochzeit = wie viel (Kochzeit | Backzeit) (muss | kann | soll) ich rechnen | was ist die (Kochzeit | Backzeit) | wie lange (kocht | backt) es | wie lange muss es (kochen | backen) | [die] Kochzeit | [die] Backzeit;&quot;;
//$gesamtzeit
dyngram = dyngram + &quot;$gesamtzeit = wie lange brauche ich | wie lange (dauert|braucht) das [Rezept] | [wie ist die] (Gesamtzeit | Gesamtdauer) | wie viel Zeit muss ich (rechnen | einplanen);&quot;;

//$schwierig
dyngram = dyngram + &quot;$schwierig = wie schwer ist (es | das Rezept) | welchen Schwierigkeitsgrad hat (es | das Rezept)| Schwierigkeitsgrad | (wie | was) ist der Schwierigkeitsgrad | Schwierigkeitsgrad | ist es schwierig ;&quot;;
//$kalorien
dyngram = dyngram + &quot;$kalorien = wie viele Kalorien hat das (Gericht | Essen) | wie hoch ist der Nährwert [des Gerichts | des Essen] | wie viel Nährwert hat das Rezept;&quot;;


//$anleitung_ganz
dyngram = dyngram + &quot;$anleitung_ganz = [sag mir | gib mir] ([alle] Schritte | [die] (komplette | ganze) Anleitung) | was (muss | soll) ich tun | Anleitung | wie geht das Rezept | wie sind die Schritte;&quot;;

//$anleitung
dyngram = dyngram + &quot;$anleitung = $next {$=[\&quot;anleitung\&quot;,\&quot;next\&quot;]} | $prev {$=[\&quot;anleitung\&quot;,\&quot;previous\&quot;]} | $first {$=[\&quot;anleitung\&quot;,\&quot;first\&quot;]} | $rep {$=[\&quot;anleitung\&quot;,\&quot;repeat\&quot;]} | $laststep {$=[\&quot;anleitung\&quot;, \&quot;last\&quot;]};&quot;;
dyngram = dyngram + &quot;$next = was ist der nächste Schritt | nächster Schritt | weiter ;&quot;;
dyngram = dyngram + &quot;$laststep = was ist der letzte Schritt | letzter Schritt | gehe ans Ende | was muss ich als letztes machen;&quot;;
dyngram = dyngram + &quot;$prev = was war der (letzte | vorherige) Schritt;&quot;;
dyngram = dyngram + &quot;$first = erster Schritt | von vorne | was ist der erste Schritt | wie geht es los | womit kann ich anfangen;&quot;;
dyngram = dyngram + &quot;$rep = was (ist | war) der Schritt | [Wiederhole (den | diesen) Schritt] nochmal;&quot;;

//$zutaten_exist
dyngram = dyngram + &quot;$zutaten_exist = (brauche ich | braucht man | brauchen wir) $term_z {$=[\&quot;exists_zutat\&quot;, $term_z]} | (kommt | kommen | gehört | gehören) $term_z (rein | hinein | dazu | dabei) {$=[\&quot;exists_zutat\&quot;, $term_z]} | ist $term_z im Gericht {$=[\&quot;exists_zutat\&quot;, $term_z]} | (gehört | gehören) $term_z zu den Zutaten {$=[\&quot;exists_zutat\&quot;, $term_z]};&quot;;
//$zutaten_menge
dyngram = dyngram + &quot;$zutaten_menge = wie viel $term_z [brauche ich] {$=[\&quot;zutaten\&quot;, $term_z]};&quot;;
//$term_z
dyngram = dyngram + &quot;$term_z = \&quot;&quot; + get(ingredients,0) + &quot;\&quot;&quot;;
for (int i = 1; i&lt;length(ingredients); i++)
  dyngram = dyngram + &quot; | \&quot;&quot; + get(ingredients,i) + &quot;\&quot;&quot;;
dyngram = dyngram + &quot;;&quot;;

//$zutaten
dyngram = dyngram + &quot;$zutaten = [sag mir | gib mir] (die Liste | alle Zutaten | die Zutatenliste | die Zutaten) | [die] Zutaten | [die] Zutatenliste | was brauche ich | Zutaten vorlesen | welche Zutaten brauche ich | [du kannst mir] [den] Einkaufszettel vorlesen;&quot;;


//$einkaufszettel
dyngram = dyngram + &quot;$einkaufszettel = [schreib mir einen] Einkaufszettel | [einen] Einkaufszettel schreiben | [eine] Einkaufsliste schreiben | [schreib mir eine] Einkaufsliste | Liste | Zettel ;&quot;;


//$help
dyngram = dyngram + &quot;$help = hilfe | was kann ich sagen | ich brauche hilfe | hilf mir (weiter) | häh | was sind meine (Möglichkeiten | Optionen) | (Was (kann | kannst) (ich | du) [nochmal | denn] (tun | machen));&quot;;</value>
						</att>
